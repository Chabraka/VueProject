n    },\r\n\r\n\r\n    methods: {\r\n\r\n        onIsLikedChanged() {\r\n    \r\n            localStorage.setItem(`this.isLiked`, this.isLiked);\r\n            this.$emit('update:isLiked', {gameId: this.id,isLiked: !this.isLiked});\r\n            console.log(document.cookie);\r\n\t\t},\r\n\r\n        /*toggleLike() {\r\n            const isLiked = !this.isLiked;     \r\n            this.$emit('update:isLiked', { gameId: this.id, isLiked });\r\n            console.log(document.cookie);\r\n        },*/\r\n\r\n\r\n    },\r\n \r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n\r\n    @import url('https://fonts.googleapis.com/css2?family=Orbitron:wght@600&display=swap');\r\n    .game-card{    \r\n        width:20rem;\r\n        text-align: center;\r\n        box-shadow: 0.1rem .2rem .4rem rgba(13, 230, 219, 0.295);\r\n        background:#fff;\r\n        margin:4rem 2rem;\r\n        background-color: rgb(15, 3, 44);\r\n        color: rgb(162, 245, 245);\r\n        margin :1.3rem;\r\n        transition: all 0.2s ease;\r\n    }\r\n\r\n    .game-card:hover{    \r\n        scale: 1.05;\r\n    }\r\n\r\n    .game-card>img{\r\n        width: 100%;\r\n    }\r\n\r\n    h3{\r\n        margin: 0.5rem;\r\n        font-family: Orbitron ;\r\n    }\r\n\r\n    .game-list{    \r\n        display: flex;\r\n        width: 31rem;\r\n        min-height: 8rem;\r\n        text-align: start;\r\n        padding: 1rem;\r\n        margin: 1rem;\r\n        box-shadow: 0.1rem .2rem .4rem rgba(13, 230, 219, 0.295);\r\n        background-color: rgb(15, 3, 44);\r\n        transition: all 0.2s ease;\r\n    }\r\n\r\n    .game-list:hover{    \r\n        scale: 1.05;\r\n    }\r\n\r\n    .image_like{\r\n        margin-right: 1rem;\r\n        display: flex;\r\n        flex-direction: column;\r\n        align-items: start;\r\n        \r\n    }\r\n\r\n    .game-list>div>img{\r\n        width:11rem;\r\n        object-fit: contain;\r\n    }\r\n\r\n    .details-game{\r\n        display: flex;\r\n        justify-content: space-between;\r\n    }\r\n\r\n    .game-list p{    \r\n        margin: 0.5rem;\r\n        font-weight: 100;\r\n        font-size: 0.8rem;\r\n    }\r\n\r\n    .like {\r\n        cursor: pointer;\r\n        height: 100px;\r\n        width: 100px;\r\n        background-image: url(https://abs.twimg.com/a/1446542199/img/t1/web_heart_animation.png);\r\n        position: absolute;\r\n        transform: translate(-40%, 75%);\r\n    }\r\n    .anim-like {\r\n        animation-name: anim-like;\r\n        animation-duration: 0.8s;\r\n        animation-timing-function: steps(28); \r\n        background-position: right;\r\n    }\r\n    @keyframes anim-like {\r\n        from{background-position: left;}\r\n        to{background-position: right;}\r\n    }\r\n\r\n   \r\n\r\n</style>\r\n\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}