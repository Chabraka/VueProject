{"ast":null,"code":"import { getCookie, setCookie } from '@/services/cookies/cookieUtils';\nexport default {\n  name: 'GameCard',\n  props: {\n    id: Number,\n    title: String,\n    thumbnail: String,\n    short_description: String,\n    genre: String,\n    platform: String,\n    release_date: Date,\n    isLiked: {\n      type: Boolean,\n      default: false\n    },\n    gridView: Boolean\n  },\n  emits: [\"update:isLiked\"],\n  created: function () {\n    const isLikedCookie = getCookie(`isLiked_${this.id}`);\n    if (isLikedCookie) {\n      this.isLiked = isLikedCookie === 'true';\n    }\n  },\n  methods: {\n    onIsLikedChanged() {\n      localStorage.setItem(`this.isLiked`, this.isLiked);\n      this.$emit('update:isLiked', {\n        gameId: this.id,\n        isLiked: !this.isLiked\n      });\n      console.log(document.cookie);\n    }\n\n    /*toggleLike() {\r\n        const isLiked = !this.isLiked;     \r\n        this.$emit('update:isLiked', { gameId: this.id, isLiked });\r\n        console.log(document.cookie);\r\n    },*/\n  }\n};","map":{"version":3,"mappings":"AA+BA,SAASA,SAAS,EAAEC,SAAQ,QAAS,gCAAgC;AAErE,eAAe;EACXC,IAAG,EAAI,UAAU;EAEjBC,KAAI,EAAI;IACJC,EAAC,EAAIC,MAAM;IACXC,KAAI,EAAIC,MAAM;IACdC,SAAQ,EAAID,MAAM;IAClBE,iBAAgB,EAAIF,MAAM;IAC1BG,KAAK,EAAEH,MAAM;IACbI,QAAQ,EAAEJ,MAAM;IAChBK,YAAY,EAAEC,IAAI;IAElBC,OAAO,EAAE;MACLC,IAAI,EAAEC,OAAO;MACbC,OAAO,EAAE;IACb,CAAC;IAEDC,QAAQ,EAAEF;EAEd,CAAC;EAEDG,KAAK,EAAE,CAAC,gBAAgB,CAAC;EAGzBC,OAAO,EAAE,YAAU;IAEf,MAAMC,aAAY,GAAIrB,SAAS,CAAE,WAAU,IAAI,CAACI,EAAG,EAAC,CAAC;IACrD,IAAIiB,aAAa,EAAE;MACf,IAAI,CAACP,OAAM,GAAIO,aAAY,KAAM,MAAM;IAC3C;EACJ,CAAC;EAGDC,OAAO,EAAE;IAELC,gBAAgB,GAAG;MAEfC,YAAY,CAACC,OAAO,CAAE,cAAa,EAAE,IAAI,CAACX,OAAO,CAAC;MAClD,IAAI,CAACY,KAAK,CAAC,gBAAgB,EAAE;QAACC,MAAM,EAAE,IAAI,CAACvB,EAAE;QAACU,OAAO,EAAE,CAAC,IAAI,CAACA;MAAO,CAAC,CAAC;MACtEc,OAAO,CAACC,GAAG,CAACC,QAAQ,CAACC,MAAM,CAAC;IACtC;;IAEM;;;;;EAOJ;AAEJ","names":["getCookie","setCookie","name","props","id","Number","title","String","t