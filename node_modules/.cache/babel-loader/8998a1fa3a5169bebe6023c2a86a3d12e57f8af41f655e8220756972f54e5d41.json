{"ast":null,"code":"import { getGamesData } from '@/services/api/gamesAPI';\nimport GameCard from './GameCard.vue';\nexport default {\n  name: 'GamesGallery',\n  components: {\n    GameCard\n  },\n  data() {\n    return {\n      gamesData: [],\n      search: '',\n      gridViews: \"true\",\n      dogsSortType: \"AZName\"\n    };\n  },\n  computed: {\n    searchingList() {\n      return this.gamesData.filter(gamesData => {\n        return gamesData.title.toLowerCase().includes(this.search.toLowerCase());\n      });\n    }\n  },\n  created: function () {\n    this.retrieveGamesData();\n  },\n  methods: {\n    async retrieveGamesData() {\n      this.gamesData = await getGamesData();\n      console.log(this.gamesData);\n    },\n    cleanSearch: function () {\n      this.search = '';\n    }\n  }\n};","map":{"version":3,"mappings":"AA0FI,SAASA,YAAW,QAAS,yBAAyB;AAEtD,OAAOC,QAAO,MAAO,gBAAgB;AAErC,eAAe;EACfC,IAAG,EAAI,cAAc;EAErBC,UAAS,EAAI;IACTF;EACJ,CAAC;EAEDG,IAAI,GAAG;IACH,OAAO;MACHC,SAAQ,EAAI,EAAE;MACdC,MAAM,EAAC,EAAE;MACTC,SAAS,EAAC,MAAM;MAChBC,YAAY,EAAE;IAClB;EACJ,CAAC;EAEDC,QAAQ,EAAE;IACNC,aAAa,GAAE;MACX,OAAO,IAAI,CAACL,SAAS,CAACM,MAAM,CAAEN,SAAS,IAAK;QACxC,OAAOA,SAAS,CAACO,KAAK,CAACC,WAAW,EAAE,CAACC,QAAQ,CAAC,IAAI,CAACR,MAAM,CAACO,WAAW,EAAE,CAAC;MAC5E,CAAC;IACL;EACJ,CAAC;EAEDE,OAAO,EAAE,YAAU;IACf,IAAI,CAACC,iBAAiB,EAAC;EAC3B,CAAC;EAEDC,OAAO,EAAE;IAEL,MAAMD,iBAAiB,GAAE;MACrB,IAAI,CAACX,SAAQ,GAAI,MAAML,YAAY,EAAC;MACpCkB,OAAO,CAACC,GAAG,CAAC,IAAI,CAACd,SAAS;IAC9B,CAAC;IAEDe,WAAW,EAAC,YAAU;MAClB,IAAI,CAACd,MAAK,GAAI,EAAC;IACzB;EACE;AAEF,CAAC","names":["getGamesData","GameCard","name","components","data","gamesData","search","gridViews","dogsSortType","computed","searchingList","filter","title","toLowerCase","includes","created","retrieveGamesData","methods","console","log","cleanSearch"],"sourceRoot":"","sources":["C:\\Users\\charl\\Documents\\IMAC\\2A\\S2\\PROG-WEB\\projet\\src\\components\\GamesGallery.vue"],"sourcesContent":["<template>\r\n\r\n    <div class=\"gallery