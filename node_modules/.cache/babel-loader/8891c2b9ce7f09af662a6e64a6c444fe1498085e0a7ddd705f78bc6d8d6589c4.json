{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { getCookie, setCookie } from '@/services/cookies/cookieUtils';\nexport default {\n  name: 'GameCard',\n  props: {\n    id: Number,\n    title: String,\n    thumbnail: String,\n    short_description: String,\n    genre: String,\n    platform: String,\n    release_date: Date,\n    gridView: Boolean\n  },\n  data() {\n    return {\n      favorites: [],\n      isLiked: localStorage.getItem(\"isLiked\") === \"false\"\n    };\n  },\n  created() {\n    const isLikedCookie = getCookie('isLiked');\n    const favoritesCookie = getCookie('favorites');\n    if (isLikedCookie) {\n      this.isLiked = isLikedCookie === 'true';\n    }\n    if (favoritesCookie) {\n      this.favorites = JSON.parse(favoritesCookie);\n    }\n  },\n  methods: {\n    toggleLike() {\n      this.isLiked = !this.isLiked;\n      if (this.isLiked) {\n        this.favorites.push(this.id);\n      } else {\n        const index = this.favorites.indexOf(this.id);\n        if (index !== -1) {\n          this.favorites.splice(index, 1);\n        }\n      }\n      Cookies.set('isLiked', this.isLiked);\n      Cookies.set('favorites', JSON.stringify(this.favorites));\n    }\n  }\n};","map":{"version":3,"mappings":";AA4BA,SAASA,SAAS,EAAEC,SAAQ,QAAS,gCAAgC;AACrE,eAAe;EACXC,IAAG,EAAI,UAAU;EAEjBC,KAAI,EAAI;IACJC,EAAC,EAAIC,MAAM;IACXC,KAAI,EAAIC,MAAM;IACdC,SAAQ,EAAID,MAAM;IAClBE,iBAAgB,EAAIF,MAAM;IAC1BG,KAAK,EAAEH,MAAM;IACbI,QAAQ,EAAEJ,MAAM;IAChBK,YAAY,EAAEC,IAAI;IAElBC,QAAQ,EAAEC;EACd,CAAC;EAIDC,IAAI,GAAG;IACH,OAAO;MACHC,SAAS,EAAE,EAAE;MAEbC,OAAO,EAAEC,YAAY,CAACC,OAAO,CAAC,SAAS,MAAM;IAEjD,CAAC;EACL,CAAC;EAEDC,OAAO,GAAG;IACZ,MAAMC,aAAY,GAAItB,SAAS,CAAC,SAAS,CAAC;IAC1C,MAAMuB,eAAc,GAAIvB,SAAS,CAAC,WAAW,CAAC;IAE9C,IAAIsB,aAAa,EAAE;MACjB,IAAI,CAACJ,OAAM,GAAII,aAAY,KAAM,MAAM;IACzC;IAEA,IAAIC,eAAe,EAAE;MACnB,IAAI,CAACN,SAAQ,GAAIO,IAAI,CAACC,KAAK,CAACF,eAAe,CAAC;IAC9C;EACF,CAAC;EAIGG,OAAO,EAAE;IAELC,UAAU,GAAG;MACT,IAAI,CAACT,OAAM,GAAI,CAAC,IAAI,CAACA,OAAO