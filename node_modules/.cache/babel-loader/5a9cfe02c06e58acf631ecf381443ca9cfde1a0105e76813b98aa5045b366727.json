{"ast":null,"code":"import { getGamesData } from '@/services/api/gamesAPI';\nimport GameCard from './GameCard.vue';\nexport default {\n  name: 'GamesGallery',\n  components: {\n    GameCard\n  },\n  watch: {\n    search: function (newSearch) {\n      localStorage.setItem(\"search\", newSearch);\n    },\n    dogsSortType: function (newDogsSortType) {\n      localStorage.setItem(\"dogsSortType\", newDogsSortType);\n    }\n  },\n  data() {\n    return {\n      gamesData: [],\n      search: '',\n      gridViews: \"true\",\n      dogsSortType: \"AZName\"\n    };\n  },\n  computed: {\n    searchingList() {\n      return this.gamesData.filter(gamesData => {\n        return gamesData.title.toLowerCase().includes(this.search.toLowerCase());\n      });\n    }\n  },\n  created: function () {\n    this.retrieveGamesData();\n  },\n  methods: {\n    async retrieveGamesData() {\n      this.gamesData = await getGamesData();\n      console.log(this.gamesData);\n    },\n    cleanSearch: function () {\n      this.search = '';\n    }\n  }\n};","map":{"version":3,"mappings":"AAgGI,SAASA,YAAW,QAAS,yBAAyB;AAEtD,OAAOC,QAAO,MAAO,gBAAgB;AAErC,eAAe;EACfC,IAAG,EAAI,cAAc;EAErBC,UAAS,EAAI;IACTF;EACJ,CAAC;EAEDG,KAAK,EAAE;IACHC,MAAM,EAAE,UAASC,SAAS,EAAE;MACxBC,YAAY,CAACC,OAAO,CAAC,QAAQ,EAAEF,SAAS;IAC5C,CAAC;IACDG,YAAY,EAAE,UAASC,eAAe,EAAC;MACnCH,YAAY,CAACC,OAAO,CAAC,cAAc,EAAEE,eAAe;IACxD;EACJ,CAAC;EAGDC,IAAI,GAAG;IACH,OAAO;MACHC,SAAQ,EAAI,EAAE;MACdP,MAAM,EAAC,EAAE;MACTQ,SAAS,EAAC,MAAM;MAChBJ,YAAY,EAAE;IAClB;EACJ,CAAC;EAEDK,QAAQ,EAAE;IACNC,aAAa,GAAE;MACX,OAAO,IAAI,CAACH,SAAS,CAACI,MAAM,CAAEJ,SAAS,IAAK;QACxC,OAAOA,SAAS,CAACK,KAAK,CAACC,WAAW,EAAE,CAACC,QAAQ,CAAC,IAAI,CAACd,MAAM,CAACa,WAAW,EAAE,CAAC;MAC5E,CAAC;IACL;EACJ,CAAC;EAEDE,OAAO,EAAE,YAAU;IACf,IAAI,CAACC,iBAAiB,EAAC;EAC3B,CAAC;EAEDC,OAAO,EAAE;IAEL,MAAMD,iBAAiB,GAAE;MACrB,IAAI,CAACT,SAAQ,GAAI,MAAMZ,YAAY,EAAC;MACpCuB,OAAO,CAACC,GAAG,CAAC,IAAI,CAACZ,SAAS;IAC9B,CAAC;IAEDa,WAAW,EAAC,YAAU;MAClB,IAAI,CAACpB,MAAK,GAAI,EAAC;IACzB;EACE;AAEF,CAAC","names"